{
  "name": "jeyroik/extas-repo-toolkit",
  "description": "Toolkit for simplifying working with Extas repositories",
  "require": {
    "jeyroik/extas-id": "0.*",
    "jeyroik/extas-exceptions": "0.*",
    "jeyroik/extas-foundation": "6.*",
    "jeyroik/extas-base": "2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "jeyroik/extas-snuffbox": "1.*"
  },
  "autoload": {
    "psr-4": {
      "extas\\": "src/",
      "tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox --coverage-clover=coverage.xml --whitelist=src tests"
  }
}